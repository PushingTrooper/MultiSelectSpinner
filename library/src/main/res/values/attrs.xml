<?xml version="1.0" encoding="utf-8"?>
<resources>
	<attr name="hintText" format="string" />
	<attr name="highlightSelected" format="boolean" />
	<attr name="highlightColor" format="color" />
	<attr name="textColor" format="color" />
	<attr name="titleColor" format="color" />
	<attr name="clearText" format="string" />

	<declare-styleable name="MultiSpinnerSearch">
		<attr name="hintText" />
		<attr name="highlightSelected" />
		<attr name="highlightColor" />
		<attr name="textColor" />
		<attr name="clearText" />
		<attr name="titleColor" />
		<attr name="selectedBackgroundColor" format="color" />
	</declare-styleable>
	<declare-styleable name="SingleSpinnerSearch">
		<attr name="hintText" />
		<attr name="highlightSelected" />
		<attr name="highlightColor" />
		<attr name="textColor" />
		<attr name="clearText" />
		<attr name="titleColor" />
	</declare-styleable>
</resources>